page {
  background-color: #efefef;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: white;
  padding: 50rpx 0;
  margin-bottom: 20rpx;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-name {
  font-size: 16px;
}

.userinfo navigator {
  display: inline;
}
