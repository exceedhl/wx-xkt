@import 'styles/weui.wxss';

button[type="primary"] {
  background-color: #00bbbd;
}

button[type="primary"][disabled] {
  background-color: #00bbbd;
}

button[type="primary"].button-hover {
  background-color: #279FA0;
}

button[type="warn"] {
  background-color: #FF654E;
}

button[type="warn"].button-hover {
  background-color: #CD4632;
}

button[type="primary"][plain] {
  color:#00bbbd;
  border:1px solid #00bbbd;
  background-color:transparent;
}

button[plain][disabled] {
  color:rgba(0, 0, 0, 0);
  border-color:rgba(0, 0, 0, 0);
}

button[disabled][type="default"], wx-button[disabled]:not([type]) {
  background-color:#FFF;
}
